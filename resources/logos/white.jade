extends _template
block template_vars
  -
    fallbackImage = fallbackImage || '/img/logos/white.png'
    options = {
      getFill: function (x, y, defaultColor, group) {
        if (group) { return defaultColor; }
        if (defaultColor === '#666666') { return '#aaaaaa'; }
        if (defaultColor === '#1a1a1a') { return '#e6e6e6'; }
        if (defaultColor === '#4d4d4d') { return '#b3b3b3'; }
        if (defaultColor === '#333333') { return '#dddddd'; }
        if (defaultColor) { return defaultColor; }
        return '#ffffff';
      }
    }
