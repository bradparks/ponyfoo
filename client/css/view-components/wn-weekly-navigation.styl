.wn-navigation
  display block
  text-align center
  background-color c-yellow-highlight-01
  margin-top -20px

.wn-list
  line-height inherit
  list-style-type none
  f-use-subheading()

.wn-item
  display inline-block

  &:after
    opacity 0.1
    content '|'

  &:last-child:after
    content none

.wn-link
  display inline-block
  margin 10px

.wl-top
  margin-top 0

.wn-navigation + .de-host .wl-top
  margin-top 35px
