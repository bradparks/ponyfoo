extends _template
block template_vars
  -
    fallbackImage = fallbackImage || '/img/logos/random-two.png'
    options = options || {}
    options.getFill = function (x, y, defaultColor, group) {
      if (group === 'eye') { return '#1bc211'; }
      if (group) { return defaultColor; }
      var random = Math.random();
      if (random > 0.75) { return '#1686a2'; }
      if (random > 0.50) { return '#1a4d7f'; }
      if (random > 0.25) { return '#900070'; }
      return '#e92c6c';
    }
